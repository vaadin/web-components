{
  "extends": "eslint:recommended",
  "rules": {
    "no-console": 0
  },
  "env": {
    "browser": true
  },
  "plugins": [
    "html"
  ],
  "globals": {
    "require": false,
    "module": false,
    "CustomElements": false,
    "HTMLImports": false,
    "Polymer": false,
    "WeakMap": false
  }
}
