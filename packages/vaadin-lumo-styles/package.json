{
  "name": "@vaadin/vaadin-lumo-styles",
  "version": "25.0.0-alpha21",
  "publishConfig": {
    "access": "public"
  },
  "description": "Lumo is a design system foundation for modern web applications, used by Vaadin components",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/vaadin/web-components.git",
    "directory": "packages/vaadin-lumo-styles"
  },
  "author": "Vaadin Ltd",
  "homepage": "https://vaadin.com/components",
  "bugs": {
    "url": "https://github.com/vaadin/web-components/issues"
  },
  "main": "all-imports.js",
  "module": "all-imports.js",
  "type": "module",
  "scripts": {
    "build": "postcss ./{lumo,utility}.css --dir dist && postcss ./presets/**.css --dir dist/presets",
    "icons": "gulp icons"
  },
  "files": [
    "*.css",
    "*.d.ts",
    "*.js",
    "!gulpfile.js",
    "!postcss.config.js",
    "components/*.css",
    "dist",
    "mixins/*.d.ts",
    "mixins/*.js",
    "presets",
    "src"
  ],
  "keywords": [
    "vaadin",
    "lumo",
    "design-system",
    "web-components",
    "web-component"
  ],
  "dependencies": {
    "@vaadin/component-base": "25.0.0-alpha21",
    "@vaadin/icon": "25.0.0-alpha21",
    "@vaadin/vaadin-themable-mixin": "25.0.0-alpha21"
  },
  "devDependencies": {
    "cssnano": "^7.1.1",
    "gulp": "^5.0.1",
    "gulp-cli": "^3.1.0",
    "gulp-iconfont": "^12.0.0",
    "gulp-plugin-extras": "^1.0.0",
    "imagemin": "^9.0.0",
    "imagemin-svgo": "^10.0.1",
    "postcss": "^8.1.0",
    "postcss-cli": "^11.0.1",
    "postcss-import": "^16.1.1"
  }
}
